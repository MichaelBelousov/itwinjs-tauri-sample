  "scripts": {
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "start": "run-p \"tauri:dev\" \"start:client\"",
    "start:client": "yarn build:backend && run-p \"start:frontend\"",
    "build": "yarn build:frontend && tauri build --debug -b deb",
    "test": "node scripts/test.js",
    "build:backend": "yarn build:backend:webpack && node scripts/move-binary.js",
    "build:backend:webpack": "webpack --config backend.webpack.config.js",
    "build:backend:watch": "webpack --config backend.webpack.config.js --watch",
    "build:frontend": "yarn pseudolocalize && node scripts/build.js",
    "//build:tauri": "pkg dist/package.json --output src-tauri/binaries/app && node scripts/move-binary.js",
    "start:frontend": "node scripts/start.js",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./public/locales/en-PSEUDO",
    "clean": "rm itwin-sidecar_*.log"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "electron 8.0.0"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "/home/mike/personal/tauri-itwinjs/node_modules/jest-circus/runner.js",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx|mjs|cjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!@bentley).+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
