  "scripts": {
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "start": "concurrently yarn:tauri:dev yarn:start:client",
    "start:client": "yarn build:backend && run-p \"start:frontend\" \"electron:debug\"",
    "build": "yarn build:backend && build:frontend && build:tauri",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:backend": "tsc -p cra-template/tsconfig.backend.json",
    "build:frontend": "yarn pseudolocalize && react-scripts build",
    "build:tauri": "pkg package.json --output src-tauri/binaries/app && node scripts/move-binary.js",
    "electron": "electron cra-template/lib/backend/main.js",
    "electron:debug": "cross-env NODE_ENV=development electron cra-template/lib/backend/main.js",
    "start:frontend": "react-scripts start",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./public/locales/en-PSEUDO"
  },
  "pkg": {
    "assets": [
      "src/**/*"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "electron 8.0.0"
